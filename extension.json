{
	"name": "BlueSpiceBookshelfUI",
	"version": "4.0.0-alpha",
	"license-name": "GPL-3.0-only",
	"url": "https://en.wiki.bluespice.com/wiki/Reference:BookshelfUI",
	"author": [
		"Robert Vogel",
		"Sebastian Ulbricht"
	],
	"descriptionmsg": "bs-bookshelfui-extension-description",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"BlueSpiceFoundation": ">= 3.3",
			"BlueSpiceBookshelf": ">= 3.3"
		}
	},
	"type": "bluespice",
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceBookshelfUI": {
					"className": "\\BlueSpice\\BookshelfUI\\Extension",
					"extPath": "/BlueSpiceBookshelfUI",
					"package": "BlueSpice Pro",
					"status": "stable"
				}
			},
			"PermissionRegistry": {
				"bookshelfbookmanager-viewspecialpage": {
					"type": "global",
					"roles": [ "reader" ]
				},
				"bookshelfbookui-viewspecialpage": {
					"type": "global",
					"roles": [ "reader" ]
				},
				"bookshelf-viewspecialpage": {
					"type": "global",
					"roles": [ "reader" ]
				}
			},
			"DynamicFileRegistry": {
				"bookshelfimage": "\\BlueSpice\\BookshelfUI\\DynamicFileDispatcher\\BookshelfImage"
			}
		},
		"BlueSpiceBookshelfUI": {
			"MassAddHandlerRegistry": {
				"subpages": "BlueSpice\\BookshelfUI\\MassAdd\\Handler\\Subpage::factory",
				"category": "BlueSpice\\BookshelfUI\\MassAdd\\Handler\\Category::factory",
				"pagecollection": "BlueSpice\\BookshelfUI\\MassAdd\\Handler\\PageCollection::factory"
			}
		}
	},
	"GroupPermissions": {
		"*": {
			"bookshelfbookmanager-viewspecialpage": true,
			"bookshelfbookui-viewspecialpage": true,
			"bookshelf-viewspecialpage": true
		}
	},
	"AvailableRights": [
		"bookshelfbookmanager-viewspecialpage",
		"bookshelfbookui-viewspecialpage",
		"bookshelf-viewspecialpage"
	],
	"config_prefix": "bsg",
	"config": {
		"BookShelfUIDefaultCoverImage": {
			"value": "resources/images/assets/coverimage/dummy.png",
			"path": true
		},
		"BookShelfUIShowChapterNavigationPagerBeforeContent": {
			"value": false
		},
		"BookShelfUIShowChapterNavigationPagerAfterContent": {
			"value": false
		}
	},
	"SpecialPages": {
		"BookshelfBookUI": "SpecialBookshelfBookUI",
		"BookshelfBookManager": "SpecialBookshelfBookManager",
		"Bookshelf": "\\BlueSpice\\BookshelfUI\\Special\\Bookshelf"
	},
	"APIModules": {
		"bs-bookshelf-manage": "ApiBookshelfManage",
		"bs-bookshelf-page-collection-store": "ApiBookshelfPageCollectionStore",
		"bs-bookshelf-mass-add-page-store": "ApiBookshelfMassAddPageStore"
	},
	"MessagesDirs": {
		"BlueSpiceBookshelfUI": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"BlueSpiceBookshelfUIAlias": "languages/BookshelfUI.alias.php"
	},
	"AutoloadNamespaces": {
		"BlueSpice\\BookshelfUI\\": "src/"
	},
	"TestAutoloadNamespaces": {
		"BlueSpice\\BookshelfUI\\Tests\\": "tests/phpunit/"
	},
	"AutoloadClasses": {
		"SpecialBookshelfBookUI": "includes/specials/SpecialBookshelfBookUI.class.php",
		"SpecialBookshelfBookManager": "includes/specials/SpecialBookshelfBookManager.class.php",
		"ApiBookshelfManage": "includes/api/ApiBookshelfManage.php",
		"ApiBookshelfPageCollectionStore": "includes/api/ApiBookshelfPageCollectionStore.php",
		"BlueSpice\\BookshelfUI\\MassAdd\\PageProvider": "includes/MassAdd/PageProvider.php",
		"ApiBookshelfMassAddPageStore": "includes/api/ApiBookshelfMassAddPageStore.php",
		"BlueSpice\\BookshelfUI\\MassAdd\\IHandler": "includes/MassAdd/IHandler.php",
		"BlueSpice\\BookshelfUI\\MassAdd\\Handler\\Subpage": "includes/MassAdd/Handler/Subpage.php",
		"BlueSpice\\BookshelfUI\\MassAdd\\Handler\\Category": "includes/MassAdd/Handler/Category.php",
		"BlueSpice\\BookshelfUI\\MassAdd\\Handler\\PageCollection": "includes/MassAdd/Handler/PageCollection.php"
	},
	"ResourceModules": {
		"ext.bluespice.bookshelfUI": {
			"messages": [
				"bs-deferred-action-status-pending",
				"bs-deferred-action-status-running",
				"bs-deferred-action-status-done",
				"bs-deferred-action-status-error",
				"bs-deferred-action-apicopypage-description",
				"bs-deferred-action-apieditpage-description",
				"bs-deferred-batch-title",
				"bs-deferred-batch-progress-desc",
				"bs-deferred-batch-actions",
				"bs-deferred-batch-description",
				"bs-deferred-batch-status",
				"bs-bookshelf-specialpage-allpages-ns-book-text"
			],
			"dependencies": [
				"mediawiki.api",
				"ext.bluespice"
			]
		},
		"ext.bluespice.bookshelfUI.pager.navigation.styles": {
			"styles": [
				"bluespice.bookshelfUI.pager.navigation.less"
			]
		},
		"ext.bluespice.bookshelfUI.pager.before-content.styles": {
			"styles": [
				"bluespice.bookshelfUI.pager.beforeContent.less"
			]
		},
		"ext.bluespice.bookshelfUI.pager.after-content.styles": {
			"styles": [
				"bluespice.bookshelfUI.pager.afterContent.less"
			]
		},
		"ext.bluespice.bookshelfUI.navigationTab.styles": {
			"styles": [
				"bluespice.bookshelfUI.navigationTab.less"
			],
			"skinStyles": {
				"bluespicecalumma": "bluespice.bookshelfUI.Calumma.less"
			}
		},
		"ext.bluespice.bookshelfUI.navigationTab": {
			"scripts": [
				"bluespice.bookshelfUI.contextMenu.js"
			],
			"messages": [
				"bs-bookshelfui-widget-editor-link-text",
				"bs-bookshelfui-grouping-template-type-ns_book"
			],
			"dependencies": [
				"ext.bluespice.bookshelfUI"
			]
		},
		"ext.bluespice.bookshelfUI.flyout": {
			"scripts": [
				"bluespice.bookshelfUI.flyout.js"
			],
			"messages": [
				"bs-bookshelfui-manager-title"
			],
			"dependencies": [
				"ext.bluespice.bookshelfUI",
				"ext.bluespice.extjs"
			]
		},
		"ext.bluespice.bookshelfUI.special": {
			"scripts": [
				"bluespice.bookshelfUI.special.js"
			],
			"dependencies": [
				"ext.bluespice.bookshelfUI.flyout"
			]
		},
		"ext.bluespice.bookshelfUI.editor": {
			"scripts": [
				"bluespice.bookshelfUI.editor.js"
			],
			"styles": [
				"bluespice.bookshelfUI.editor.css",
				"bluespice.bookshelfUI.icons.css"
			],
			"messages": [
				"bs-bookshelfui-export-selection",
				"bs-bookshelfui-save-and-modify",
				"bs-bookshelfui-dlg-choosewikipage-title",
				"bs-bookshelfui-dlg-choosewikipage-cbxArticleLabel",
				"bs-bookshelfui-dlg-choosewikipage-tfDisplayName",
				"bs-bookshelfui-dlg-choosecategory-label",
				"bs-bookshelfui-ctxmnu-open",
				"bs-bookshelfui-ctxmnu-edit",
				"bs-bookshelfui-ctxmnu-delete",
				"bs-bookshelfui-ctxmnu-tag-edit",
				"bs-bookshelfui-dlg-metadata-title",
				"bs-bookshelfui-dlg-tagsettings-title",
				"bs-bookshelfui-confirm-delete-title",
				"bs-bookshelfui-confirm-delete-text",
				"bs-bookshelfui-override-tag-title",
				"bs-bookshelfui-override-tag-text",
				"bs-bookshelfui-nothingtoexport-text",
				"bs-bookshelfui-bookui-savingprogress-title",
				"bs-bookshelfui-bookui-savingprogress-text",
				"bs-bookshelfui-empty-selection",
				"bs-bookshelfui-extjs-tooltip-add-mass",
				"bs-bookshelfui-dlg-addmass-title",
				"bs-bookshelfui-dlg-type-label",
				"bs-bookshelfui-type-category",
				"bs-bookshelfui-type-subpages",
				"bs-bookshelfui-dlg-choosepc-label",
				"bs-bookshelfui-type-pagecollection",
				"bs-bookshelfui-export-book-text"
			],
			"dependencies": [
				"ext.bluespice.bookshelfUI",
				"ext.bluespice.extjs"
			]
		},
		"ext.bluespice.bookshelfUI.manager": {
			"scripts": "bluespice.bookshelfUI.manager.js",
			"messages": [
				"bs-bookshelfui-new-book-title",
				"bs-bookshelfui-new-book-text",
				"bs-bookshelfui-delete-book-title",
				"bs-bookshelfui-delete-book-text",
				"bs-bookshelfui-manager-title",
				"bs-bookshelfui-manager-deletingprogress-text",
				"bs-bookshelfui-manager-deletingprogress-title",
				"bs-bookshelfui-manager-deletionsuccess-title",
				"bs-bookshelfui-manager-deletionsuccess-text",
				"bs-bookshelfui-manager-deletionfailure-title",
				"bs-bookshelfui-manager-deletionfailure-text",
				"bs-bookshelfui-dlg-copy-title",
				"bs-bookshelfui-dlg-copy-target-name-label",
				"bs-bookshelfui-dlg-copy-name-addition",
				"bs-bookshelfui-dlg-copy-target-namespace-qtip",
				"bs-bookshelfui-grouping-template-books",
				"bs-bookshelfui-grouping-template-type-ns_book",
				"bs-bookshelfui-grouping-template-type-user_book",
				"bs-bookshelfui-book-type",
				"bs-bookshelf-personal-books-page-prefix"
			],
			"dependencies": [
				"mediawiki.Title",
				"ext.bluespice.bookshelfUI",
				"ext.bluespice.bookshelfUI.extjs.i18n",
				"ext.bluespice.extjs"
			]
		},
		"ext.bluespice.bookshelfUI.extjs.i18n": {
			"messages": [
				"bs-bookshelfui-extjs-tooltip-copy"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceBookshelfUI/resources"
	},
	"Hooks": {
		"ChameleonSkinTemplateOutputPageBeforeExec": [
			"\\BlueSpice\\BookshelfUI\\Hook\\SkinTemplateOutputPageBeforeExec\\AddChapterPager::callback",
			"\\BlueSpice\\BookshelfUI\\Hook\\ChameleonSkinTemplateOutputPageBeforeExec\\AddBookshelfUI::callback"
		],
		"BeforePageDisplay": "\\BlueSpice\\BookshelfUI\\Hook\\BeforePageDisplay\\AddResources::callback",
		"GetPreferences": "\\BlueSpice\\BookshelfUI\\Hook\\GetPreferences\\BookshelfDefaultView::callback"
	},
	"manifest_version": 2,
	"DefaultUserOptions": {
		"bs-bookshelfui-defaultview": "gridviewpanel"
	},
	"load_composer_autoloader": true
}
